{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": true
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
      "@typescript-eslint/require-await": "off",
      "@typescript-eslint/no-unused-vars": "warn"
    }
}
